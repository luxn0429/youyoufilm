    <async-support>true</async-support>
    <init-param>
      <param-name>remote</param-name>
      <param-value>false</param-value>
    </init-param>
  </filter>
  <filter-mapping>
     <filter-name>TestFilter</filter-name>
     <url-pattern>/*</url-pattern>
  </filter-mapping>
  -->
</web-app>


